@model ClientsDirectoryApp.Models.ClientInfo

<h2>Client Details</h2>
<table class="table">
    <tr><td>Client ID:</td><td>@Model.ClientId</td></tr>
    <tr><td>Company Name:</td><td>@Model.CompanyName</td></tr>
    <tr><td>Web URL:</td><td>@Model.WebUrl</td></tr>
    <tr><td>Email:</td><td>@Model.Email</td></tr>
    <tr><td>Category:</td><td>@Model.Category</td></tr>
    <tr><td>Standard:</td><td>@Model.Standard</td></tr>
</table>
